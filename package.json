{
  "name": "sub-graph-proxy",
  "license": "UNLICENSED",
  "scripts": {
    "lint": "eslint .",
    "generate-chapel": "yarn codegen:demix-chapel && yarn build:demix-chapel",
    "generate-bsc": "yarn codegen:demix-bsc && yarn build:demix-bsc",
    "generate-xdai": "yarn codegen:demix-xdai && yarn build:demix-xdai",
    "generate-matic": "yarn codegen:demix-matic && yarn build:demix-matic",
    "generate-mainnet": "yarn codegen:demix-mainnet && yarn build:demix-mainnet",
    "generate-goerli": "yarn codegen:demix-goerli && yarn build:demix-goerli",
    "generate-avalanche": "yarn codegen:demix-avalanche && yarn build:demix-avalanche",
    "generate-arbitrum": "yarn codegen:demix-arbitrum && yarn build:demix-arbitrum",
    "generate-optimism": "yarn codegen:demix-optimism && yarn build:demix-optimism",
    "generate-cronos": "yarn codegen:demix-cronos && yarn build:demix-cronos",
    "generate-fantom": "yarn codegen:demix-fantom && yarn build:demix-fantom",
    "yaml:demix-chapel": "node ./create-yaml-file create-yaml -e chapel | mustache - mustache/yaml.mustache > subgraphs/demix-subgraph-chapel.yaml",
    "yaml:demix-bsc": "node ./create-yaml-file create-yaml -e bsc | mustache - mustache/yaml.mustache > subgraphs/demix-subgraph-bsc.yaml",
    "yaml:demix-xdai": "node ./create-yaml-file create-yaml -e xdai | mustache - mustache/yaml.mustache > subgraphs/demix-subgraph-xdai.yaml",
    "yaml:demix-matic": "node ./create-yaml-file create-yaml -e matic | mustache - mustache/yaml.mustache > subgraphs/demix-subgraph-matic.yaml",
    "yaml:demix-mainnet": "node ./create-yaml-file create-yaml -e mainnet | mustache - mustache/yaml.mustache > subgraphs/demix-subgraph-mainnet.yaml",
    "yaml:demix-goerli": "node ./create-yaml-file create-yaml -e goerli | mustache - mustache/yaml.mustache > subgraphs/demix-subgraph-goerli.yaml",
    "yaml:demix-avalanche": "node ./create-yaml-file create-yaml -e avalanche | mustache - mustache/yaml.mustache > subgraphs/demix-subgraph-avalanche.yaml",
    "yaml:demix-arbitrum": "node ./create-yaml-file create-yaml -e arbitrum-one | mustache - mustache/yaml.mustache > subgraphs/demix-subgraph-arbitrum.yaml",
    "yaml:demix-optimism": "node ./create-yaml-file create-yaml -e optimism | mustache - mustache/yaml.mustache > subgraphs/demix-subgraph-optimism.yaml",
    "yaml:demix-cronos": "node ./create-yaml-file create-yaml -e cronos | mustache - mustache/yaml.mustache > subgraphs/demix-subgraph-cronos.yaml",
    "yaml:demix-fantom": "node ./create-yaml-file create-yaml -e fantom | mustache - mustache/yaml.mustache > subgraphs/demix-subgraph-fantom.yaml",
    "codegen": "graph codegen",
    "codegen:demix-chapel": "yarn yaml:demix-chapel && yarn codegen -- subgraphs/demix-subgraph-chapel.yaml",
    "codegen:demix-bsc": "yarn yaml:demix-bsc && yarn codegen -- subgraphs/demix-subgraph-bsc.yaml",
    "codegen:demix-xdai": "yarn yaml:demix-xdai && yarn codegen -- subgraphs/demix-subgraph-xdai.yaml",
    "codegen:demix-matic": "yarn yaml:demix-matic && yarn codegen -- subgraphs/demix-subgraph-matic.yaml",
    "codegen:demix-mainnet": "yarn yaml:demix-mainnet && yarn codegen -- subgraphs/demix-subgraph-mainnet.yaml",
    "codegen:demix-goerli": "yarn yaml:demix-goerli && yarn codegen -- subgraphs/demix-subgraph-goerli.yaml",
    "codegen:demix-avalanche": "yarn yaml:demix-avalanche && yarn codegen -- subgraphs/demix-subgraph-avalanche.yaml",
    "codegen:demix-arbitrum": "yarn yaml:demix-arbitrum && yarn codegen -- subgraphs/demix-subgraph-arbitrum.yaml",
    "codegen:demix-optimism": "yarn yaml:demix-optimism && yarn codegen -- subgraphs/demix-subgraph-optimism.yaml",
    "codegen:demix-cronos": "yarn yaml:demix-cronos && yarn codegen -- subgraphs/demix-subgraph-cronos.yaml",
    "codegen:demix-fantom": "yarn yaml:demix-fantom && yarn codegen -- subgraphs/demix-subgraph-fantom.yaml",
    "build": "graph build",
    "build:demix-chapel": "graph build subgraphs/demix-subgraph-chapel.yaml",
    "build:demix-bsc": "graph build subgraphs/demix-subgraph-bsc.yaml",
    "build:demix-xdai": "graph build subgraphs/demix-subgraph-xdai.yaml",
    "build:demix-matic": "graph build subgraphs/demix-subgraph-matic.yaml",
    "build:demix-mainnet": "graph build subgraphs/demix-subgraph-mainnet.yaml",
    "build:demix-goerli": "graph build subgraphs/demix-subgraph-goerli.yaml",
    "build:demix-avalanche": "graph build subgraphs/demix-subgraph-avalanche.yaml",
    "build:demix-arbitrum": "graph build subgraphs/demix-subgraph-arbitrum.yaml",
    "build:demix-optimism": "graph build subgraphs/demix-subgraph-optimism.yaml",
    "build:demix-cronos": "graph build subgraphs/demix-subgraph-cronos.yaml",
    "build:demix-fantom": "graph build subgraphs/demix-subgraph-fantom.yaml",
    "deploy": "graph deploy --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ --access-token $TOKEN",
    "deploy:demix-chapel": "yarn deploy -- decentraworlddewo/bsc-testnet-demix-subgraph subgraphs/demix-subgraph-chapel.yaml",
    "deploy:demix-bsc": "yarn deploy -- decentraworlddewo/bsc-demix-subgraphx subgraphs/demix-subgraph-bsc.yaml",
    "deploy:demix-xdai": "yarn deploy -- decentraworlddewo/xdai-demix-subgraph subgraphs/demix-subgraph-xdai.yaml",
    "deploy:demix-matic": "yarn deploy -- decentraworlddewo/matic-demix-subgraph subgraphs/demix-subgraph-matic.yaml",
    "deploy:demix-mainnet": "yarn deploy -- decentraworlddewo/mainnet-demix-subgraph subgraphs/demix-subgraph-mainnet.yaml",
    "deploy:demix-goerli": "yarn deploy -- decentraworlddewo/goerli-demix-subgraph subgraphs/demix-subgraph-goerli.yaml",
    "deploy:demix-avalanche": "yarn deploy -- decentraworlddewo/avalanche-demix-subgraph subgraphs/demix-subgraph-avalanche.yaml",
    "deploy:demix-arbitrum": "yarn deploy -- decentraworlddewo/arbitrum-demix-subgraph subgraphs/demix-subgraph-arbitrum.yaml",
    "deploy:demix-optimism": "yarn deploy -- decentraworlddewo/optimism-demix-subgraph subgraphs/demix-subgraph-optimism.yaml",
    "deploy:demix-cronos": "yarn deploy -- decentraworlddewo/cronos-demix-subgraph subgraphs/demix-subgraph-cronos.yaml",
    "deploy:demix-fantom": "yarn deploy -- decentraworlddewo/fantom-demix-subgraph subgraphs/demix-subgraph-fantom.yaml"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "0.20.0",
    "@graphprotocol/graph-ts": "0.20.0",
    "node-api-version": "^0.1.4",
    "yarn-api": "^1.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "commander": "^7.2.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "mustache": "^4.2.0",
    "prettier": "^2.3.1",
    "typescript": "^4.3.2"
  }
}
